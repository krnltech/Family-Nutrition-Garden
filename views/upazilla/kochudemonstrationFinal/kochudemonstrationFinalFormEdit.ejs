<%- include('../../includes/header')-%>
        <div class="wrapper">
          <%- include('../../includes/topbar')-%>
            <div class="page-wrap">
              <%- include('../../includes/sidebar')-%>
              <div class="main-content">
                <div class="container-fluid">
                    <div class="row clearfix">
                    <div id="signupbox" style="margin-top:50px" class="mainbox col-md-6 col-md-offset-3 col-sm-8 col-sm-offset-2">
                        <div class="panel panel-info">
                            <div class="panel-heading">
                                <div class="panel-title"><h2>স্যাঁতস্যাঁতে জমিতে কচু জাতীয়
                                    সবজি চাষ প্রদর্শনী এর চূড়ান্ত প্রতিবেদন ফর্ম</h2></div>
                                <p><%
                                if(success !=''){ %>
                                <%= success %>
                                    <% } %>
                                </p>
                                </div>  
                            <div class="panel-body" >
                                <form action="/upazilla/kochudemonstrationFinalFormEditPost/<%= records.id %>" id="FieldTrialForm" class="form-horizontal" method="post"  role="form">
                                    <div id="signupalert" style="display:none" class="alert alert-danger">
                                        <p>Error:</p>
                                        <span></span>
                                    </div>
                                    <div class="form-group">
                                        <label for="bdate" class="col-md-3 control-label">সব্জি ও ফসল কর্তনের তারিখ</label>
                                        <div class="col-md-9">
                                            <input type="date" class="form-control" name="sobjiDate" placeholder="সব্জি ও ফসল কর্তনের তারিখ" value="<%=records.sobjiDate%>">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="comment" class="col-md-3 control-label">প্রদর্শনী প্লটের ফলন(কেজি)</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="pfolon" placeholder="প্রদর্শনী প্লটের ফলন(কেজি)" value="<%=records.pfolon%>">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="bdate" class="col-md-3 control-label">ফলন/হেঃ (মেঃ টন)</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="hfolon" placeholder="ফলন/হেঃ (মেঃ টন)" value="<%=records.hfolon%>">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="comment" class="col-md-3 control-label">কৃষকের মন্তব্য</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="fcomment" placeholder="কৃষকের মন্তব্য" value="<%=records.fcomment%>">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="comment" class="col-md-3 control-label">কর্মকর্তার মন্তব্য</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="kcomment" placeholder="কর্মকর্তার মন্তব্য" value="<%=records.kcomment%>">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <div class="col-md-9">
                                            <input type="hidden" class="form-control" name="user_id" placeholder="id" value="<%= user_id %>">
                                        </div>
                                    </div>
                                    
                                    <div class="form-group">
                                        <!-- Button -->                                        
                                        <div class="col-md-offset-3 col-md-9">
                                        <input type="submit" name="submit" value="submit" class="btn btn-primary">   
                                        </div>
                                    </div>
                                    
                                    
                                        
                                    
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
<%- include('../../includes/footer')-%>