<%if (records.length >0){ %>
    <table class="table table-bordered table-striped" id="tblCustomers" cellspacing="0" cellpadding="0">
    <thead>
    <tr>
        <th>ক্রমিক নং</th>
        <th>সেমিনারের বিষয়</th>
        <th>সেমিনারের তারিখ ও স্থান</th>
        <th>অংশগ্রহণকারীর নাম ও পদবী</th>
        <th>অংশগ্রহণকারীর কর্মস্থল</th>
        <th>অংশগ্রহণকারীর মোবাইল নং </th>
        <th>রিসোর্স পারসনের নাম ও পদবী</th>
        <th>উপজেলা কৃষি অফিসারের মন্তব্য</th>
        <th>উপ-পরিচালকের মন্তব্য</th>
        <th>প্রকল্প পরিচালকের মন্তব্য</th>
        <th>মন্তব্য করুন</th>
    </tr>
    </thead>
    <tbody>
        <% records.forEach(function(row,index){ %>
            <tr>
                <td><%= index+1 %></td>
                <td><%= row.topic %></td>
                <td><%= row.date %></td>
                <td><%= row.name%></td>
                <td><%= row.place %></td>
                <td><%= row.mobile %></td>
                <td><%= row.resource%></td>
                <td><%= row.comment %></td>
                <td><%= row.ddComment %></td>
                <td><%= row.pdComment %></td>
                <td><a class="btn btn-primary btn-xs" href="/dd/agriFairEdit/<%= row.id %>"><i class="fa fa-edit"></i></a>
                </td>
            </tr>
        <% }) %>
    </tbody>
</table>
<% }else{ %>
    <table class="table table-bordered table-striped" id="tblCustomers" cellspacing="0" cellpadding="0">
        <thead>
        <tr>
            <th>ক্রমিক নং</th>
            <th>সেমিনারের বিষয়</th>
            <th>সেমিনারের তারিখ ও স্থান</th>
            <th>অংশগ্রহণকারীর নাম ও পদবী</th>
            <th>অংশগ্রহণকারীর কর্মস্থল</th>
            <th>অংশগ্রহণকারীর মোবাইল নং </th>
            <th>রিসোর্স পারসনের নাম ও পদবী</th>
            <th>উপজেলা কৃষি অফিসারের মন্তব্য</th>
            <th>উপ-পরিচালকের মন্তব্য</th>
            <th>প্রকল্প পরিচালকের মন্তব্য</th>
            <th>পরিবর্তন/মুছুন</th>
        </tr>
        </thead>
        <tbody>
        <% records.forEach(function(row){ %>
            <tr>
                <td colspan="10">No data Found</td>
            </tr>
        <% }) %>
        </tbody>
    </table>
<% } %>