<%- include('../../includes/header')-%>
        <div class="wrapper">
          <%- include('../../includes/topbar')-%>
            <div class="page-wrap">
              <%- include('../../includes/sidebar')-%>
              <div class="main-content">
                <div class="container-fluid">
                    <div class="row clearfix">
                    <div id="signupbox" style="margin-top:50px" class="mainbox col-md-6 col-md-offset-3 col-sm-8 col-sm-offset-2">
                        <div class="panel panel-info">
                            <div class="panel-heading">
                                <div class="panel-title"><h2>সেমিনার তথ্য ফর্ম</h2></div>
                                <p><%
                                if(success !=''){ %>
                                <%= success %>
                                    <% } %>
                                </p>
                                </div>  
                            <div class="panel-body" >
                                <form action="/upazilla/agriFairEditPost/<%=records.id%>" id="FieldTrialForm" class="form-horizontal" method="post"  role="form">
                                    <div id="signupalert" style="display:none" class="alert alert-danger">
                                        <p>Error:</p>
                                        <span></span>
                                    </div>

   
                                    <div class="form-group">
                                        <label for="booth" class="col-md-3 control-label">সেমিনারের বিষয়</label>
                                        <div class="col-md-9">
                                        <input type="text" class="form-control" name="topic" placeholder="সেমিনারের বিষয়" value="<%=records.topic%>">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="technology" class="col-md-3 control-label">সেমিনারের তারিখ ও স্থান</label>
                                        <div class="col-md-9">
                                            <input type="date" class="form-control" name="date" placeholder="সেমিনারের তারিখ ও স্থান" value="<%=records.date%>">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="name" class="col-md-3 control-label">অংশগ্রহণকারীর নাম ও পদবী</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="name" placeholder="অংশগ্রহণকারীর নাম ও পদবী" value="<%=records.name%>">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="place" class="col-md-3 control-label">অংশগ্রহণকারীর কর্মস্থল</label>
                                        <div class="col-md-9">
                                        <input type="text" class="form-control" name="place" placeholder="অংশগ্রহণকারীর কর্মস্থল" value="<%=records.place%>">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="mobile" class="col-md-3 control-label">অংশগ্রহণকারীর মোবাইল নং</label>
                                        <div class="col-md-9">
                                            <input type="date" class="form-control" name="mobile" placeholder="অংশগ্রহণকারীর মোবাইল নং" value="<%=records.mobile%>">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="resource" class="col-md-3 control-label">রিসোর্স পারসনের নাম ও পদবী</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="resource" placeholder="রিসোর্স পারসনের নাম ও পদবী" value="<%=records.resource%>">
                                        </div>
                                    </div>

                                    
                                
                                    <div class="form-group">
                                        <label for="comment" class="col-md-3 control-label">উপজেলা কৃষি অফিসারের মন্তব্য</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="comment" placeholder="উপজেলা কৃষি অফিসারের মন্তব্য " value="<%=records.comment%>">
                                        </div>
                                    </div>


                                    <div class="form-group">
                                        <div class="col-md-9">
                                            <input type="hidden" class="form-control" name="user_id" placeholder="id" value="<%= user_id %>">
                                        </div>
                                    </div>
                                    
                                    <div class="form-group">
                                        <!-- Button -->                                        
                                        <div class="col-md-offset-3 col-md-9">
                                        <input type="submit" name="submit" value="submit" class="btn btn-primary">   
                                        </div>
                                    </div>
                                    
                                    
                                        
                                    
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
<%- include('../../includes/footer')-%>